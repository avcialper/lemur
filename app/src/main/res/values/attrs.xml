<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="backgroundColor" format="color" />
    <attr name="primaryColor" format="color" />
    <attr name="secondaryColor" format="color" />
    <attr name="bottomMenuColor" format="color" />
    <attr name="inputAreaBackgroundColor" format="color" />
    <attr name="teamHeaderColor" format="color" />
    <declare-styleable name="TextInput">
        <attr name="android:autofillHints" />
        <attr name="android:hint" />
        <attr name="android:inputType" />
        <attr name="errorIconDrawable" />
        <attr name="endIconTint" />
        <attr name="endIconMode" />
        <attr name="have_counter" format="boolean" />
        <attr name="counter_max_length" format="integer" />
        <attr name="android:imeOptions" />
    </declare-styleable>
    <declare-styleable name="IconLabel">
        <attr name="android:label" format="string" />
        <attr name="left_icon" format="reference" />
    </declare-styleable>
    <declare-styleable name="TasksArea">
        <attr name="empty_text" format="string" />
    </declare-styleable>
    <declare-styleable name="Tasks">
        <attr name="empty_text" />
        <attr name="is_short" format="boolean" />
        <attr name="have_margin" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomFAB">
        <attr name="fab_count" format="enum">
            <enum name="two" value="2" />
            <enum name="four" value="4" />
        </attr>
        <attr name="fab_icon" format="reference" />
        <attr name="first_fab_text" format="string" />
        <attr name="second_fab_text" format="string" />
        <attr name="third_fab_text" format="string" />
        <attr name="fourth_fab_text" format="string" />
    </declare-styleable>
    <declare-styleable name="HeaderAndDivider">
        <attr name="label" format="string" />
    </declare-styleable>
    <declare-styleable name="EmptyArea">
        <attr name="contentText" format="string" />
        <attr name="buttonText" format="string" />
    </declare-styleable>
</resources>